{
  "extends": "airbnb",
  "plugins": [
    "babel",
    "jsx-a11y"
  ],
  "env": {
    "node": true
  },
  "rules": {
    "no-console": "off",
    "strict": "off",
    "maxlen": 0,
    "max-len": 0,
    "func-names": "off",
    "linebreak-style": "off",
    "no-param-reassign": 0,
    "arrow-body-style": 0,
    "no-plusplus": "off",
    "jsx-a11y/href-no-hash": "off"
  }
}
